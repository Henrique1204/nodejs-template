{
  "name": "nodejs-template",
  "version": "0.0.1",
  "description": "Template para projetos com NodeJS + TypeScript.",
  "main": "src/server.ts",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "test": "jest",
    "test:silent": "jest --watchAll --silent --noStackTrace",
    "test:coverage": "jest --coverage --silent --noStackTrace"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Henrique1204/nodejs-template.git"
  },
  "keywords": [],
  "author": "Paulo Henrique",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Henrique1204/nodejs-template/issues"
  },
  "homepage": "https://github.com/Henrique1204/nodejs-template#readme",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "swagger-ui-express": "^5.0.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/cors": "^2.8.13",
    "@types/node-cache": "^4.2.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
